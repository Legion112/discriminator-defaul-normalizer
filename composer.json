{
    "name": "legion112/discriminator-default-normalizer",
    "description": "Symfony serializer normalizer for normalizing to parent object in case of not found DiscriminatorMap",
    "type": "library",
    "license": "MIT",
    "keywords": ["symfony", "serializer", "normalizer"],
    "require": {
        "symfony/serializer": "^6.1",
        "php": "^8.1",
        "symfony/property-access": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26"
    },
    "autoload": {
        "psr-4": {
            "Legion112\\SerializerDiscriminatorDefault\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
